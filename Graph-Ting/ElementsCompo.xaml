<UserControl x:Class="Graph_Ting.ElementsCompo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Graph_Ting"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="200">
    <StackPanel>
        <Label Content="Enter Non negative Integer here"></Label>
        <Label Content="Number must from 0 to 999"></Label>
        <TextBox x:Name="IntBox" FontSize="14"></TextBox>
        <Button x:Name="Add" Content="Add Number" Click="AddNumberButton_Click" Margin="0,10,0,5" FontSize="18"></Button>
        <ListBox x:Name="NumbersBox" MinHeight="50" Margin="0,0,0,10"></ListBox>
        <Button x:Name="Remove" Content="Remove Number" Click="RemoveNumberButton_Click" Margin="0,10,0,5" FontSize="16"></Button>
    </StackPanel>
</UserControl>
